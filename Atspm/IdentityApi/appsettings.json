  "Swagger": {
    "Title": "UtahId OIDC",
    "Description": "OpenId Api with JWT UtahId Tokens",
    "Contact": {
      "Name": "udotdevelopment",
      "Email": "udotdevelopment@gmail.com",
      "Url": "https://udottraffic.utah.gov/atspm/"
    },
    "License": {
      "Name": "Apache",
      "Url": "http://www.apache.org/licenses/LICENSE-2."
    }
  },
  "SwaggerUI": {
    "DocExpansion": "List", //List, Full, None
    "DefaultModelsExpandDepth": 0, //-1 to hide models, 0 to collapse, 1+ to expand
    "DisplayRequestDuration": true
  },
  "ApiVersioning": {
    "DefaultMajorVersion": "1",
    "DefaultMinorVersion": "0",
    "SunsetPolicies": [
      {
        "MajorVersion": "0",
        "MinorVersion": "1",
        "EffectiveDays": 60,
        "Link": "https://example.com/sunset",
        "Title": "These are only available during development",
        "Type": "text/html"
      },
      {
        "MajorVersion": "0",
        "MinorVersion": "9",
        "EffectiveDays": 60,
        "Link": "https://example.com/sunset",
        "Title": "This is just a test",
        "Type": "text/html"
      }
    ]
  },
  "AllowedHosts": "*",
  "Jwt": {
    "ExpireDays": 1,
    "Key": "ATSPMProductionIdentityOpenSource2024",
    "Issuer": "AvenueConsultants"
  },
  "EmailConfiguration": {
    "SmtpEmailService": {
      "Host": "localhost",
      "Port": 25,
      "UserName": "dummy",
      "Password": "dummy",
      "EnableSsl": false
    }
  },
  "ConnectionStrings": {
    "ConfigContext": {
      "Provider": "SqlServer",
      "ConnectionString": "Server=localhost,1433;Database=ATSPM_Config;Trusted_Connection=True;TrustServerCertificate=True;MultipleActiveResultSets=True;"
    },
    "AggregationContext": {
      "Provider": "SqlServer",
      "ConnectionString": "Server=localhost,1433;Database=ATSPM_Aggregation;Trusted_Connection=True;TrustServerCertificate=True;MultipleActiveResultSets=True;"
    },
    "EventLogContext": {
      "Provider": "SqlServer",
      "ConnectionString": "Server=localhost,1433;Database=ATSPM_EventLog;Trusted_Connection=True;TrustServerCertificate=True;MultipleActiveResultSets=True;"
    },
    "IdentityContext": {
      "Provider": "SqlServer",
      "ConnectionString": "Server=localhost,1433;Database=ATSPM_Identity;Trusted_Connection=True;TrustServerCertificate=True;MultipleActiveResultSets=True;"
    }
  },
  "CorsPolicies": {
    "Default": {
      "Origins": [ "http://localhost:3000" ],
      "Methods": [ "*" ],
      "Headers": [ "*" ],
      "AllowCredentials": true
    }
  }
}