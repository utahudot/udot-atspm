    "@types/jest": "^29.5.12",
    "@types/js-cookie": "^3.0.6",
    "@types/leaflet": "^1.9.3",
    "@types/mapbox__polyline": "^1.0.5",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/sanitize-html": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
