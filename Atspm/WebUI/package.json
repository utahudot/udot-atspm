  "eslintConfig": {
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "next/core-web-vitals",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "ignorePatterns": [
      "next.config.js",
      "src/mocks/data/*"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "ignoreRestSiblings": true
        }
      ],
      "@typescript-eslint/no-explicit-any": "warn"
    }
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ],
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
