{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "Phase Termination",
  "description": "Phase Termination chart",
  "type": "object",
  "properties": {
    "chartName": { "type": "string" },
    "signalId": { "type": "string" },
    "signalLocation": { "type": "string" },
    "phaseNumber": { "type": "integer" },
    "phaseDescription": { "type": "string" },
    "start": { "type": "string" },
    "end": { "type": "string" },
    "consecutiveCount": { "type": "integer" },
    "plans": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "planNumber": { "type": "string" },
          "startTime": { "type": "string" },
          "endTime": { "type": "string" }
        }
      }
    },
    "gapOuts": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": "string",
          "phaseNumber": "integer"
        }
      }
    },
    "maxOuts": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": "string",
          "phaseNumber": "integer"
        }
      }
    },
    "forceOffs": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": "string",
          "phaseNumber": "integer"
        }
      },
      "pedWalkBegins": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "startTime": "string",
            "phaseNumber": "number"
          }
        },
        "unknownTerminations": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "startTime": "string",
              "phaseNumber": "integer"
            }
          }
        }
      }
