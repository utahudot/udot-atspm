{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "Split Fail",
  "description": "Split Fail chart",
  "type": "object",
  "properties": {
    "chartName": { "type": "string" },
    "signalId": { "type": "string" },
    "signalLocation": { "type": "string" },
    "phaseNumber": { "type": "integer" },
    "phaseDescription": { "type": "string" },
    "start": { "type": "string" },
    "end": { "type": "string" },
    "totalSplitFails": { "type": "integer" },
    "plans": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "planNumber": { "type": "string" },
          "startTime": { "type": "string" },
          "endTime": { "type": "string" },
          "fails": { "type": "string" },
          "percentFails": { "type": "string" }
        }
      }
    },
    "failLines": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "number": { "type": "integer" }
        }
      }
    },
    "gapOutGreenOccupancies": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "percent": { "type": "number" }
        }
      }
    },
    "gapOutRedOccupancies": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "percent": { "type": "number" }
        }
      }
    },
    "forceOffGreenOccupancies": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "seconds": { "type": "number" }
        }
      }
    },
    "forceOffRedOccupancies": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "percent": { "type": "number" }
        }
      }
    },
    "averageGor": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "average": { "type": "number" }
        }
      }
    },
    "averageRor": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "average": { "type": "number" }
        }
      }
    },
    "percentFails": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "percent": { "type": "number" }
        }
      }
    }
  }
}
  

