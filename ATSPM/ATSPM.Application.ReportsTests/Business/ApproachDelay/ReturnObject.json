{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "Arrival On Red",
  "description": "Arrival On Red chart",
  "type": "object",
  "properties": {
    "chartName": { "type": "string" },
    "signalId": { "type": "string" },
    "signalLocation": { "type": "string" },
    "phaseNumber": { "type": "integer" },
    "phaseDescription": { "type": "string" },
    "start": { "type": "string" },
    "end": {"type": "string"},
    "totalArrivalOnRed": { "type": "integer" },
    "totalDetectorHits": { "type": "integer" },
    "percentArrivalOnRed": { "type": "number" },
    "plans": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "percentArrivalOnGreen": { "type": "number" },
          "percentGreenTime": { "type": "number" },
          "platoonatio": { "type": "number" },
          "planNumber": { "type": "string" },
          "startTime": { "type": "string" },
          "endTime": { "type": "string" }
        }
      }
    },
    "percentArrivalsOnRed": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": "string",
          "percent": "number"
        }
      }
    },
    "totalVehicles": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": "string",
          "vehicles": "number"
        }
      }
    },
    "arrivalsOnRed": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": "string",
          "arrivals": "number"
        }
      }
    }
  }
