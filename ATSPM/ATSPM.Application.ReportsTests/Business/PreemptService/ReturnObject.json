{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "Preempt Service",
  "description": "Preempt Service chart",
  "type": "object",
  "properties": {
    "chartName": { "type": "string" },
    "signalId": { "type": "string" },
    "signalLocation": { "type": "string" },
    "start": { "type": "string" },
    "end": { "type": "string" },
    "plans": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "planNumber": { "type": "string" },
          "startTime": { "type": "string" },
          "endTime": { "type": "string" },
          "preemptCount": { "type": "int" }
        }
      }
    },
    "preemptServiceEvents": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": "string",
          "eventParam": "integer"
        }
      }
    }
  }
}
  

