{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "Preempt Service Request",
  "description": "Preempt Service Request chart",
  "type": "object",
  "properties": {
    "chartName": { "type": "string" },
    "signalLocation": { "type": "string" },
    "preemptMessage": { "type": "string" },
    "plans": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "planNumber": { "type": "string" },
          "startTime": { "type": "string" },
          "endTime": { "type": "string" }
        }
      }
    },
    "preemptRequests": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": "string",
          "eventParam": "integer"
        }
      }
    }
      }
    }
  

