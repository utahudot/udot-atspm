{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "Turning Movement Count",
  "description": "Turning Movement Count chart",
  "type": "object",
  "properties": {
    "chartName": { "type": "string" },
    "signalId": { "type": "string" },
    "signalLocation": { "type": "string" },
    "start": { "type": "string" },
    "end": { "type": "string" },
    "direction": { "type": "string" },
    "movementType": { "type": "string" },
    "laneType": { "type": "string" },
    "plans": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "planNumber": { "type": "string" },
          "startTime": { "type": "string" },
          "endTime": { "type": "string" }
        }
      }
    },
    "lane": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "volume": { "type": "integer" }
        }
      }
    },
    "thruLeft": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "volume": { "type": "integer" }
        }
      }
    },
    "thruRight": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "volume": { "type": "number" }
        }
      }
    },
    "totalVolume": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "volume": { "type": "integer" }
        }
      }
    }
  }
}
  

