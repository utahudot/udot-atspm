{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "Preempt Detail",
  "description": "Preempt Detail chart",
  "type": "object",
  "properties": {
    "chartName": { "type": "string" },
    "signalLocation": { "type": "string" },
    "preemptMessage": { "type": "string" },
    "plans": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "planNumber": { "type": "string" },
          "startTime": { "type": "string" },
          "endTime": { "type": "string" }
        }
      }
    },
    "delay": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": "string",
          "seconds": "number"
        }
      }
    },
    "serviceTimes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": "string",
          "seconds": "number"
        }
      }
    },
    "trackClearTimes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": "string",
          "seconds": "double"
        }
      }
    },
      "dwellTimes": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "startTime": "string",
            "seconds": "number"
          }
        }
      },
        "callMaxOutTimes": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "startTime": "string",
              "seconds": "number"
            }
          }
        },
        "gateDownTimes": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "startTime": "string",
              "seconds": "number"
            }
          }
        },
        "inputOns": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "startTime": "string",
              "seconds": "number"
            }
          }
        },
        "inputOffs": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "startTime": "string",
              "seconds": "number"
            }
          }
        }
      }
    }
  

