{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "Approach Volume",
  "description": "Approach Volume Chart",
  "type": "object",
  "properties": {
    "chartName": { "type": "string" },
    "signalId": { "type": "string" },
    "signalLocation": { "type": "string" },
    "phaseNumber": { "type": "integer" },
    "phaseDescription": { "type": "string" },
    "start": { "type": "string" },
    "end": { "type": "string" },
    "detectorType": { "type": "string" },
    "distanceFromStopBar": { "type": "integer" },
    "plans": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "planNumber": { "type": "string" },
          "startTime": { "type": "string" },
          "endTime": { "type": "string" },
          "averageSpeed": { "type": "integer" },
          "standardDeviation": { "type": "integer" },
          "eightyFifthPercentile": { "type": "integer" },
          "fifteenthPercentile": { "type": "integer" }
        }
      }
    },
    "primaryDirectionName": { "type": "string" },
    "primaryDirectionVolumes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "volume": { "type": "integer" }
        }
      }
    },
    "opposingDirectionName": { "type": "string" },
    "opposingDirectionVolumes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "volume": { "type": "integer" }
        }
      }
    },
    "combinedDirectionVolumes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "volume": { "type": "integer" }
        }
      }
    },
    "primaryDFactors": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "dFactor": { "type": "number" }
        }
      }
    },
    "opposingDFactors": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "startTime": { "type": "string" },
          "dFactor": { "type": "number" }
        }
      }
    }
  }
}
