{
  "UseGoogleLogger": "false",
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Error",
      "Microsoft.EntityFrameworkCore": "Warning",
      "utah.udot.atspm": "Warning"
    }
  },
  "LocationControllerLoggerConfiguration": {
    "SaveToDatabaseBatchSize": 50000,
    "MaxDegreeOfParallelism": 50,
    "BulkCopyTimeout": 60
  },
  "DeviceDownloaderConfiguration": {
    "DeviceDownloader": {
      "LocalPath": "C:\\temp",
      "DeleteFile": "false",
      "Ping": "true"
    }
  },
  "LocationControllerDecoderConfiguration": {
    "ASCLocationControllerDecoder": {
      "DeleteFile": "true",
      "EarliestAcceptableDate": "1970/01/01"
    },
    "MaxTimeLocationControllerDecoder": {
      "DeleteFile": "true",
      "EarliestAcceptableDate": "1970/01/01"
    }
  },
  "FileRepositoryConfiguration": {
    "RootPath": "C:\\Repositories"
  }
}