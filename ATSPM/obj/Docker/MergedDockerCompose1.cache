services:
  signalcontrollerlogger:
    build:
      context: C:\Projects\udot-atsmp\ATSPM
      dockerfile: SignalControllerLogger/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: SignalControllerLogger
      target: base
    container_name: SignalControllerLogger
    entrypoint: tail -f /dev/null
    environment:
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: signalcontrollerlogger:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/SignalControllerLogger.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    tty: true
    volumes:
    - C:\Projects\udot-atsmp\ATSPM\SignalControllerLogger:/app:rw
    - C:\Users\christianbaker\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\christianbaker\.nuget\packages:/root/.nuget/packages:ro
version: '3.4'